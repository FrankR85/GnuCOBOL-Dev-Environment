Grammar

    0 $accept: statement_list "end of file"

    1 statement_list: %empty
    2               | statement_list statement

    3 statement: copy_statement "."
    4          | replace_statement "."
    5          | directive "end of line"
    6          | listing_statement
    7          | CONTROL_STATEMENT control_options _dot "end of line"

    8 directive: SOURCE_DIRECTIVE source_directive
    9          | DEFINE_DIRECTIVE define_directive
   10          | SET_DIRECTIVE set_directive
   11          | TURN_DIRECTIVE turn_directive
   12          | LISTING_DIRECTIVE listing_directive
   13          | LEAP_SECOND_DIRECTIVE leap_second_directive

   14 $@1: %empty

   15 directive: IF_DIRECTIVE $@1 if_directive

   16 $@2: %empty

   17 directive: ELIF_DIRECTIVE $@2 if_directive
   18          | ELSE_DIRECTIVE
   19          | ENDIF_DIRECTIVE

   20 $@3: %empty

   21 directive: CALL_DIRECTIVE $@3 call_directive

   22 set_directive: set_choice
   23              | set_directive set_choice

   24 set_choice: CONSTANT "Variable" "Literal"
   25           | "Variable" set_options
   26           | ADDRSV alnum_list
   27           | ADDSYN alnum_equality
   28           | COMP1 "Literal"
   29           | FOLDCOPYNAME _as "Literal"
   30           | MAKESYN alnum_equality
   31           | NOFOLDCOPYNAME
   32           | OVERRIDE alnum_equality_list
   33           | REMOVE alnum_list
   34           | SOURCEFORMAT _as "Literal"

   35 alnum_list: "Literal"
   36           | alnum_list "Literal"

   37 alnum_equality_list: alnum_equality
   38                    | alnum_equality_list alnum_equality

   39 alnum_equality: "Literal" "=" "Literal"

   40 set_options: %empty
   41            | _as "Literal"

   42 source_directive: _format _is format_type

   43 format_type: FIXED
   44            | FREE
   45            | VARIABLE
   46            | "word"

   47 define_directive: "Variable" _as "Literal" _override
   48                 | "Variable" _as PARAMETER _override
   49                 | "Variable" _as OFF
   50                 | CONSTANT "Variable" _as "Literal" _override
   51                 | variable_or_literal

   52 listing_directive: %empty
   53                  | ON
   54                  | OFF

   55 listing_statement: LISTING_STATEMENT
   56                  | TITLE_STATEMENT "Literal" _dot "end of line"

   57 control_options: control_option
   58                | control_options control_option

   59 control_option: SOURCE
   60               | NOSOURCE
   61               | LIST
   62               | NOLIST
   63               | MAP
   64               | NOMAP

   65 _dot: %empty
   66     | "."

   67 leap_second_directive: %empty
   68                      | ON
   69                      | OFF

   70 turn_directive: ec_list CHECKING on_or_off

   71 ec_list: "Variable"
   72        | ec_list "Variable"

   73 on_or_off: %empty
   74          | OFF
   75          | ON with_loc
   76          | with_loc

   77 with_loc: WITH LOCATION
   78         | LOCATION

   79 call_directive: call_choice
   80               | call_directive call_choice

   81 call_choice: COBOL
   82            | "EXTERN"
   83            | STDCALL
   84            | STATIC

   85 if_directive: "Variable" _is _not DEFINED
   86             | "Variable" _is _not SET
   87             | "Variable" _is _not condition_clause object_id
   88             | "Literal" _is _not condition_clause object_id
   89             | variable_or_literal

   90 variable_or_literal: "Variable"
   91                    | "Literal"

   92 object_id: "Literal"
   93          | "Variable"

   94 condition_clause: GREATER _than OR EQUAL _to
   95                 | GREATER _than
   96                 | LESS _than OR EQUAL _to
   97                 | LESS _than
   98                 | EQUAL _to
   99                 | ">="
  100                 | ">"
  101                 | "<="
  102                 | "<"
  103                 | "="
  104                 | "<>"

  105 copy_statement: COPY "Identifier or Literal" copy_in copy_suppress copy_replacing

  106 copy_in: %empty
  107        | in_or_of "Identifier or Literal"

  108 in_or_of: IN
  109         | OF

  110 copy_suppress: %empty
  111              | SUPPRESS _printing

  112 copy_replacing: %empty
  113               | REPLACING replacing_list

  114 replace_statement: REPLACE _also replacing_list
  115                  | REPLACE _last OFF

  116 replacing_list: text_src BY text_dst
  117               | lead_trail text_partial_src BY text_partial_dst
  118               | replacing_list text_src BY text_dst
  119               | replacing_list lead_trail text_partial_src BY text_partial_dst

  120 text_src: "==" token_list "=="
  121         | identifier

  122 text_dst: "==" "=="
  123         | "==" token_list "=="
  124         | identifier

  125 text_partial_src: "==" "Identifier or Literal" "=="

  126 text_partial_dst: "==" "=="
  127                 | "==" "Identifier or Literal" "=="

  128 token_list: "Identifier or Literal"
  129           | token_list "Identifier or Literal"

  130 identifier: "Identifier or Literal"
  131           | identifier IN "Identifier or Literal"
  132           | identifier OF "Identifier or Literal"
  133           | identifier '(' subscripts ')'

  134 subscripts: "Identifier or Literal"
  135           | subscripts "Identifier or Literal"

  136 lead_trail: LEADING
  137           | TRAILING

  138 _override: %empty
  139          | OVERRIDE

  140 _not: %empty
  141     | NOT

  142 _also: %empty
  143      | ALSO

  144 _last: %empty
  145      | LAST

  146 _as: %empty
  147    | AS

  148 _format: %empty
  149        | FORMAT

  150 _is: %empty
  151    | IS

  152 _printing: %empty
  153          | PRINTING

  154 _than: %empty
  155      | THAN

  156 _to: %empty
  157    | TO


